      <div class="container">
        <div class="row">
          <div class="col-sm-12 col-md-12 col-lg-12">
            <span class="pagetitle__subheading">Book A Table</span>
            <h1 class="pagetitle__heading">Reservation</h1>
          </div><!-- /.col-lg-12 -->
        </div><!-- /.row -->
      </div><!-- /.container -->
    </section><!-- /.page-title -->

    <!-- ========================
      Reservation
    =========================== -->
    <section id="reservation" class="reservation">
      <div class="container">
        <div class="row">
          <div class="col-sm-12 col-md-12 col-lg-6 offset-lg-3">
            <div class="heading text-center mb-30">
              <p class="heading__desc">You can book a table online easily in just a minute. Reservations are for lunch
                and dinner, check the availability of your table & book it now!</p>
            </div><!-- /.heading -->
          </div><!-- /.col-lg-6 -->
        </div><!-- /.row -->
        <div class="row">
          <div class="col-sm-12 col-md-12 col-lg-8 offset-lg-2">
            <form>
              <div class="row">
                <div class="col-sm-4 col-md-4 col-lg-4">
                  <div class="form-group">
                    <select class="form-control">
                      <option value="2">2 people</option>
                      <option value="3">3 people</option>
                      <option value="4">4 people</option>
                      <option value="5">5 people</option>
                      <option value="6">6 people</option>
                      <option value="7">7 people</option>
                    </select>
                  </div>
                </div><!-- /.col-lg-4 -->
                <div class="col-sm-4 col-md-4 col-lg-4">
                  <div class="form-group">
                    <select class="form-control">
                      <option value="23">March 23, 2020</option>
                      <option value="24">March 24, 2020</option>
                      <option value="25">March 25, 2020</option>
                      <option value="26">March 26, 2020</option>
                    </select>
                  </div>
                </div><!-- /.col-lg-4 -->
                <div class="col-sm-4 col-md-4 col-lg-4">
                  <div class="form-group">
                    <select class="form-control">
                      <option value="8">8:00 PM</option>
                      <option value="9">9:00 PM</option>
                      <option value="10">10:00 PM</option>
                      <option value="11">11:00 PM</option>
                    </select>
                  </div>
                </div><!-- /.col-lg-4 -->
                <div class="col-sm-12 col-md-12 col-lg-12">
                  <button type="submit" class="btn btn__primary btn__block">Find A Table</button>
                </div><!-- /.col-lg-12 -->
              </div><!-- /.row -->
            </form>
          </div><!-- /.col-lg-8 -->
        </div><!-- /.row -->
      </div><!-- /.container -->
    </section><!-- /.Reservation -->

    <!-- =========================== 
      portfolio Gallery 
    ============================= -->
    <section id="portfolioGallery" class="portfolio portfolio-gallery portfolio-gallery-no-margin py-0">
      <div class="container-fluid col-padding-0">
        <div class="row">
            @foreach(var item in Model) {
                <div class="col-sm-6 col-md-4 col-lg-3">
                    <div class="portfolio-item">
                        <div class="portfolio__img">
                            <img src="@item.Path" alt="portfolio img">
                        </div>
                        <div class="portfolio__hover">
                            <a href="@item.Path" data-lightbox="lightbox" class="zoom__icon"></a>
                        </div>
                    </div>
                </div>
            }
        </div><!-- /.row -->
      </div><!-- /.container -->
    </section><!-- /.portfolio Gallery -->
